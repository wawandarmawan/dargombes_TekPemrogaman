#include <iostream>
using namespace std;

main()
{
 long int barang, harga, pesanan;
 cout<<"menu makanan"<<endl;
 cout<<"1. sabun Rp 5000"<<endl;
 cout<<"2. shampo Rp 10000"<<endl;
 cout<<"3. rinso Rp 15000"<<endl;
 cout<<"id_barang=";
 cin>>barang;
 switch (barang)
 {
  case 1:
   cout<<"pesanan anda=sabun"<<endl;
   harga = 5000;
  break;
  case 2:
   cout<<"pesanan anda=shampo"<<endl;
    harga = 10000;
  break;
  case 3:
   cout<<"pesanan anda=rinso"<<endl;
  harga = 15000;
  break;
   default:
  cout<< "tidak tersedia"<<endl;
 }
 cout<<"jumlah pesanan=";
 cin>>pesanan;
 cout<<"total yang harus dibayar="<<harga*pesanan;
}
