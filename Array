#include <iostream>
using namespace std;

main()
{
 long int barang, harga, pembelian;
 cout<<"list harga barang"<<endl;
 cout<<"1. sabun : 3000"<<endl;
 cout<<"2. shampo : 500"<<endl;
 cout<<"3. rinso : 5000"<<endl;
 cout<<"id_barang=";
 cin>>barang;
 switch (barang)
 {
  case 1:
   cout<<"pembelian=sabun"<<endl;
   harga = 3000;
  break;
  case 2:
   cout<<"pembelian=shampo"<<endl;
    harga = 500;
  break;
  case 3:
   cout<<"pembelian=rinso"<<endl;
  harga = 5000;
  break;
   default:
  cout<< "tidak tersedia"<<endl;
 }
 cout<<"jumlah pembelian=";
 cin>>pembelian;
 cout<<"jumlah bayar="<<harga*pembelian;
}
